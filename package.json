{
  "name": "quick-lint",
  "version": "1.0.0",
  "description": "Unified code quality orchestrator for React â€” ESLint, Prettier, Husky, Commitlint & SonarQube analysis in one package with zero configuration.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "bin",
    "templates",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "node --import tsx --test test/**/*.test.ts",
    "lint": "node ./bin/quick-lint.js lint",
    "clean": "rimraf dist && npm cache clean --force",
    "clean:pack": "npm run clean && npm run build && npm pack",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "eslint",
    "prettier",
    "husky",
    "commitlint",
    "sonarqube",
    "sonarjs",
    "react",
    "typescript",
    "code-quality",
    "linter",
    "formatter",
    "orchestrator"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}