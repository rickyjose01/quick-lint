{
  "name": "quicklint-react",
  "version": "1.0.1",
  "description": "Unified code quality orchestrator for React â€” ESLint, Prettier, Husky, Commitlint & SonarQube analysis in one package with zero configuration.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "bin": {
    "quicklint": "./bin/quicklint.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./eslint-config": {
      "import": "./dist/defaults/eslint.config.js"
    },
    "./prettier-config": {
      "import": "./dist/defaults/prettier.config.js"
    },
    "./commitlint-config": {
      "import": "./dist/defaults/commitlint.config.js"
    }
  },
  "files": [
    "dist",
    "bin",
    "templates",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "node --import tsx --test test/**/*.test.ts",
    "lint": "node ./bin/quicklint.js lint",
    "clean": "rimraf dist && npm cache clean --force",
    "clean:pack": "npm run clean && npm run build && npm pack",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "eslint",
    "prettier",
    "husky",
    "commitlint",
    "sonarqube",
    "sonarjs",
    "react",
    "typescript",
    "code-quality",
    "linter",
    "formatter",
    "orchestrator"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "cosmiconfig": "^9.0.0",
    "deepmerge": "^4.3.1",
    "globals": "^15.15.0",
    "ora": "^8.2.0"
  },
  "peerDependencies": {
    "@commitlint/cli": ">=19.0.0",
    "@commitlint/config-conventional": ">=19.0.0",
    "@commitlint/lint": ">=19.0.0",
    "@commitlint/load": ">=19.0.0",
    "@typescript-eslint/eslint-plugin": ">=8.0.0",
    "@typescript-eslint/parser": ">=8.0.0",
    "eslint": ">=9.0.0",
    "eslint-plugin-react": ">=7.0.0",
    "eslint-plugin-react-hooks": ">=5.0.0",
    "eslint-plugin-jsx-a11y": ">=6.0.0",
    "eslint-plugin-sonarjs": ">=3.0.0",
    "husky": ">=9.0.0",
    "prettier": ">=3.0.0",
    "typescript-eslint": ">=8.0.0"
  },
  "peerDependenciesMeta": {
    "husky": {
      "optional": true
    },
    "prettier": {
      "optional": true
    },
    "@commitlint/cli": {
      "optional": true
    },
    "@commitlint/config-conventional": {
      "optional": true
    },
    "@commitlint/lint": {
      "optional": true
    },
    "@commitlint/load": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-jsx-a11y": {
      "optional": true
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@commitlint/lint": "^19.6.0",
    "@commitlint/load": "^19.6.1",
    "@types/node": "^22.13.4",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "eslint": "^9.20.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-sonarjs": "^3.0.2",
    "husky": "^9.1.7",
    "prettier": "^3.5.2",
    "rimraf": "^6.1.3",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}